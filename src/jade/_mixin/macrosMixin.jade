//- BTN
mixin btn(className, classNameModify, textName)
    a(
        href="javascript:void(0);"
        title=""
        class="btn " + className + " " + classNameModify
    )
        span.btn__text!= textName
        span.btn__icon-wrap
            i.btn__icon


//- MAIN TITLE
mixin mainTitle(text)
    p.main__title!= text